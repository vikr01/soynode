{
  "ignore": ["node_modules"],
  "presets": [
    [
      "@babel/preset-env",
      {
        "targets": {
          "node": 10
        },
        "useBuiltIns": "usage"
      }
    ]
  ],
  "plugins": [
    "@babel/plugin-transform-flow-comments",
    "@babel/plugin-proposal-class-properties",
    "dynamic-import-node"
  ]
}
